@prefix : <http://fdri.ceh.ac.uk/vocab/metadata/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://fdri.ceh.ac.uk/vocab/metadata/> .

<http://fdri.ceh.ac.uk/vocab/metadata> rdf:type owl:Ontology ;
                                        owl:imports <http://www.opengis.net/ont/geosparql/1.1> ,
                                                    <http://www.w3.org/2006/time#2016> ,
                                                    <http://www.w3.org/ns/dcat3> ,
                                                    <http://www.w3.org/ns/ssn/ext> ,
                                                    <https://w3id.org/iadopt/ont/1.0.3> ;
                                        rdfs:comment "An onotology for the recording of dataset metadata, provenance information and related reference data for use in the fine-grained metadata store element of the FDRI project."@en ;
                                        rdfs:label "FDRI Fine-grained Metadata Store Ontology"@en ;
                                        owl:versionInfo "0.3 DRAFT (In Progress)" .

#################################################################
#    Object Properties
#################################################################

###  http://fdri.ceh.ac.uk/vocab/metadata/affectedSystem
:affectedSystem rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                owl:inverseOf :hadFault ;
                rdfs:domain :Fault ;
                rdfs:range :EnvironmentalMonitoringSystem .


###  http://fdri.ceh.ac.uk/vocab/metadata/affectedVariable
:affectedVariable rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf owl:topObjectProperty ;
                  rdfs:domain :Fault ;
                  rdfs:range :ComplexObservableProperty .


###  http://fdri.ceh.ac.uk/vocab/metadata/annotationPropertyScheme
:annotationPropertyScheme rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf <http://www.w3.org/ns/dcat#themeTaxonomy> ;
                          rdfs:range :ComplexObservablePropertyScheme .


###  http://fdri.ceh.ac.uk/vocab/metadata/appliesToSystem
:appliesToSystem rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:range :EnvironmentalMonitoringSystem .


###  http://fdri.ceh.ac.uk/vocab/metadata/appliesToVariable
:appliesToVariable rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   rdfs:range :ComplexObservableProperty .


###  http://fdri.ceh.ac.uk/vocab/metadata/catchmentScheme
:catchmentScheme rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf <http://www.w3.org/ns/dcat#themeTaxonomy> ;
                 rdfs:range :CatchmentScheme .


###  http://fdri.ceh.ac.uk/vocab/metadata/certification
:certification rdf:type owl:ObjectProperty .


###  http://fdri.ceh.ac.uk/vocab/metadata/configuration
:configuration rdf:type owl:ObjectProperty ;
               rdfs:range :ConfigurationValueSeries .


###  http://fdri.ceh.ac.uk/vocab/metadata/configurationPropertyScheme
:configurationPropertyScheme rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf <http://www.w3.org/ns/dcat#themeTaxonomy> ;
                             rdfs:range :ConfigurationPropertyScheme .


###  http://fdri.ceh.ac.uk/vocab/metadata/environmentalDomain
:environmentalDomain rdf:type owl:ObjectProperty ;
                     rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> .


###  http://fdri.ceh.ac.uk/vocab/metadata/hadConfigurationItem
:hadConfigurationItem rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf owl:topObjectProperty ;
                      rdfs:range :ConfigurationItem .


###  http://fdri.ceh.ac.uk/vocab/metadata/hadFault
:hadFault rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty .


###  http://fdri.ceh.ac.uk/vocab/metadata/hadValue
:hadValue rdf:type owl:ObjectProperty ;
          rdfs:range :TimeBoundPropertyValue .


###  http://fdri.ceh.ac.uk/vocab/metadata/hasAnnotation
:hasAnnotation rdf:type owl:ObjectProperty ;
               rdfs:range :Annotation .


###  http://fdri.ceh.ac.uk/vocab/metadata/hasCurrentConfigurationItem
:hasCurrentConfigurationItem rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf owl:topObjectProperty ;
                             rdfs:range :ConfigurationItem .


###  http://fdri.ceh.ac.uk/vocab/metadata/hasCurrentValue
:hasCurrentValue rdf:type owl:ObjectProperty ;
                 rdfs:range :TimeBoundPropertyValue .


###  http://fdri.ceh.ac.uk/vocab/metadata/hasProcedure
:hasProcedure rdf:type owl:ObjectProperty ;
              rdfs:range :Procedure .


###  http://fdri.ceh.ac.uk/vocab/metadata/hasUnit
:hasUnit rdf:type owl:ObjectProperty ;
         rdfs:domain :ComplexObservableProperty ;
         rdfs:range <http://qudt.org/schema/qudt/Unit> ;
         rdfs:comment "Relates a Complex Observable Property to the unit of measurement used by observations of the property."@en .


###  http://fdri.ceh.ac.uk/vocab/metadata/hasValue
:hasValue rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:range <http://schema.org/PropertyValue> .


###  http://fdri.ceh.ac.uk/vocab/metadata/hasValueScheme
:hasValueScheme rdf:type owl:ObjectProperty ;
                rdfs:range <http://www.w3.org/2004/02/skos/core#ConceptScheme> .


###  http://fdri.ceh.ac.uk/vocab/metadata/hasValueSeries
:hasValueSeries rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdfs:range :PropertyValueSeries .


###  http://fdri.ceh.ac.uk/vocab/metadata/inCatchment
:inCatchment rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :temporallyValidProperty .


###  http://fdri.ceh.ac.uk/vocab/metadata/inRegion
:inRegion rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :temporallyValidProperty .


###  http://fdri.ceh.ac.uk/vocab/metadata/interval
:interval rdf:type owl:ObjectProperty ;
          rdfs:range <http://purl.org/dc/terms/PeriodOfTime> .


###  http://fdri.ceh.ac.uk/vocab/metadata/landCover
:landCover rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :temporallyValidProperty .


###  http://fdri.ceh.ac.uk/vocab/metadata/method
:method rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf owl:topObjectProperty ;
        rdfs:domain <http://www.w3.org/2004/02/skos/core#Concept> .


###  http://fdri.ceh.ac.uk/vocab/metadata/monitoringFacilityTypeScheme
:monitoringFacilityTypeScheme rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf <http://www.w3.org/ns/dcat#themeTaxonomy> ;
                              rdfs:range :EnvironmentalMonitoringFacilityTypeScheme .


###  http://fdri.ceh.ac.uk/vocab/metadata/monitoringSystemTypeScheme
:monitoringSystemTypeScheme rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf <http://www.w3.org/ns/dcat#themeTaxonomy> ;
                            rdfs:range :EnvironmentalMonitoringFacilityTypeScheme .


###  http://fdri.ceh.ac.uk/vocab/metadata/observedPropertyScheme
:observedPropertyScheme rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf <http://www.w3.org/ns/dcat#themeTaxonomy> ;
                        rdfs:range :ComplexObservablePropertyScheme .


###  http://fdri.ceh.ac.uk/vocab/metadata/operatingPeriod
:operatingPeriod rdf:type owl:ObjectProperty ;
                 rdfs:range <http://purl.org/dc/terms/PeriodOfTime> .


###  http://fdri.ceh.ac.uk/vocab/metadata/phenomenonInterval
:phenomenonInterval rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    rdfs:range <http://purl.org/dc/terms/PeriodOfTime> .


###  http://fdri.ceh.ac.uk/vocab/metadata/programme
:programme rdf:type owl:ObjectProperty ;
           rdfs:range :EnvironmentalMonitoringProgramme .


###  http://fdri.ceh.ac.uk/vocab/metadata/property
:property rdf:type owl:ObjectProperty ;
          rdfs:range :ComplexObservableProperty .


###  http://fdri.ceh.ac.uk/vocab/metadata/qualifier
:qualifier rdf:type owl:ObjectProperty ;
           rdfs:range :Annotation .


###  http://fdri.ceh.ac.uk/vocab/metadata/regionScheme
:regionScheme rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://www.w3.org/ns/dcat#themeTaxonomy> ;
              rdfs:range :RegionScheme .


###  http://fdri.ceh.ac.uk/vocab/metadata/relativeLocation
:relativeLocation rdf:type owl:ObjectProperty ;
                  rdfs:range :RelativeLocation .


###  http://fdri.ceh.ac.uk/vocab/metadata/serviceContactDetails
:serviceContactDetails rdf:type owl:ObjectProperty ;
                       rdfs:range <http://www.w3.org/2006/vcard/ns#Kind> .


###  http://fdri.ceh.ac.uk/vocab/metadata/soilType
:soilType rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:range :SoilType .


###  http://fdri.ceh.ac.uk/vocab/metadata/status
:status rdf:type owl:ObjectProperty .


###  http://fdri.ceh.ac.uk/vocab/metadata/temporallyValidProperty
:temporallyValidProperty rdf:type owl:ObjectProperty ;
                         rdfs:range :PropertyValueSeries .


###  http://fdri.ceh.ac.uk/vocab/metadata/trackLog
:trackLog rdf:type owl:ObjectProperty .


###  http://fdri.ceh.ac.uk/vocab/metadata/valueStatistic
:valueStatistic rdf:type owl:ObjectProperty ;
                rdfs:range :ValueStatistic .


###  http://fdri.ceh.ac.uk/vocab/metadata/wasModifiedBy
:wasModifiedBy rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <http://www.w3.org/ns/prov#wasInfluencedBy> ;
               rdfs:domain :ObservationDataset .


###  http://purl.org/dc/terms/replaces
<http://purl.org/dc/terms/replaces> rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/type
<http://purl.org/dc/terms/type> rdf:type owl:ObjectProperty .


###  http://schema.org/unit
<http://schema.org/unit> rdf:type owl:ObjectProperty ;
                         rdfs:domain <http://qudt.org/schema/qudt/Unit> .


###  http://schema.org/valueReference
<http://schema.org/valueReference> rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/prov#wasInfluencedBy
<http://www.w3.org/ns/prov#wasInfluencedBy> rdfs:subPropertyOf <http://www.w3.org/ns/ssn/wasOriginatedBy> .


###  http://www.w3.org/ns/sosa/hasFeatureOfInterest
<http://www.w3.org/ns/sosa/hasFeatureOfInterest> <http://schema.org/domainIncludes> :EnvironmentalMonitoringFacility .


###  http://www.w3.org/ns/sosa/observedProperty
<http://www.w3.org/ns/sosa/observedProperty> <http://schema.org/domainIncludes> :ObservationDataset .


#################################################################
#    Data properties
#################################################################

###  http://fdri.ceh.ac.uk/vocab/metadata/aggregationPeriod
:aggregationPeriod rdf:type owl:DatatypeProperty ;
                   rdfs:range xsd:duration .


###  http://fdri.ceh.ac.uk/vocab/metadata/altitude
:altitude rdf:type owl:DatatypeProperty ;
          rdfs:range xsd:decimal ;
          rdfs:comment "Height above sea-level measured in metres."@en .


###  http://fdri.ceh.ac.uk/vocab/metadata/assetNumber
:assetNumber rdf:type owl:DatatypeProperty ;
             rdfs:range xsd:string .


###  http://fdri.ceh.ac.uk/vocab/metadata/dateOfDisposal
:dateOfDisposal rdf:type owl:DatatypeProperty ;
                rdfs:range xsd:date .


###  http://fdri.ceh.ac.uk/vocab/metadata/dateOfPurchase
:dateOfPurchase rdf:type owl:DatatypeProperty ;
                rdfs:range xsd:date .


###  http://fdri.ceh.ac.uk/vocab/metadata/deployedDepth
:deployedDepth rdf:type owl:DatatypeProperty ;
               rdfs:range xsd:decimal ;
               rdfs:comment "The depth (in metres) below ground level at which an environmental monitoring system is deployed."@en .


###  http://fdri.ceh.ac.uk/vocab/metadata/deployedHeight
:deployedHeight rdf:type owl:DatatypeProperty ;
                rdfs:range xsd:decimal ;
                rdfs:comment "The height (in metres) above ground level at which an environmental monitoring system is deployed."@en .


###  http://fdri.ceh.ac.uk/vocab/metadata/deploymentPosition
:deploymentPosition rdf:type owl:DatatypeProperty ;
                    rdfs:range rdf:langString ;
                    rdfs:comment "Descriptive information about the positioning of an Environmental Monitoring System as the result of the containing StaticDeployment activity"@en .


###  http://fdri.ceh.ac.uk/vocab/metadata/deploymentVariance
:deploymentVariance rdf:type owl:DatatypeProperty ;
                    rdfs:range rdf:langString ;
                    rdfs:comment "Descriptive information about the way in which the deployment of an Environmental Monitoring System differs from the standard deployment of a system of its type."@en .


###  http://fdri.ceh.ac.uk/vocab/metadata/elevation
:elevation rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf owl:topDataProperty ;
           rdfs:range xsd:decimal ;
           rdfs:comment "The height offset of a relative location from its origin, measured in metres."@en .


###  http://fdri.ceh.ac.uk/vocab/metadata/isMobile
:isMobile rdf:type owl:DatatypeProperty ;
          rdfs:range xsd:boolean .


###  http://fdri.ceh.ac.uk/vocab/metadata/offsetEast
:offsetEast rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty ;
            rdfs:range xsd:decimal ;
            rdfs:comment "The easterly offset of a relative location from its origin, measured in metres."@en .


###  http://fdri.ceh.ac.uk/vocab/metadata/offsetNorth
:offsetNorth rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:range xsd:decimal ;
             rdfs:comment "The northerly offset of a relative location from its origin measured in metres."@en .


###  http://fdri.ceh.ac.uk/vocab/metadata/procedurePeriodicity
:procedurePeriodicity rdf:type owl:DatatypeProperty ;
                      rdfs:range xsd:duration ;
                      rdfs:comment "How frequently a process or procedure should be carried out."@en .


###  http://fdri.ceh.ac.uk/vocab/metadata/removeData
:removeData rdf:type owl:DatatypeProperty ;
            rdfs:range xsd:boolean .


###  http://fdri.ceh.ac.uk/vocab/metadata/repository
:repository rdf:type owl:DatatypeProperty ;
            rdfs:range xsd:anyURI ;
            <http://schema.org/domainIncludes> <http://www.w3.org/ns/prov#SoftwareAgent> ;
            rdfs:comment "The IRI of the source control repository which holds the source code for a prov:SoftwareAgent."@en ;
            rdfs:label "source control repository"@en .


###  http://fdri.ceh.ac.uk/vocab/metadata/repositoryPath
:repositoryPath rdf:type owl:DatatypeProperty ;
                rdfs:label "source control repository path"@en .


###  http://fdri.ceh.ac.uk/vocab/metadata/serialNumber
:serialNumber rdf:type owl:DatatypeProperty ;
              rdfs:range xsd:string .


###  http://fdri.ceh.ac.uk/vocab/metadata/siteVariance
:siteVariance rdf:type owl:DatatypeProperty ;
              rdfs:range rdf:langString ;
              rdfs:comment "Descriptive information about the way in which the layout of an Environmental Monitoring Facility differs from the standard layout for a facility of its type."@en .


###  http://fdri.ceh.ac.uk/vocab/metadata/storageLocation
:storageLocation rdf:type owl:DatatypeProperty ;
                 rdfs:range xsd:string .


###  http://fdri.ceh.ac.uk/vocab/metadata/unitName
:unitName rdf:type owl:DatatypeProperty ;
          rdfs:domain :ComplexObservableProperty ;
          rdfs:range rdf:langString ;
          rdfs:comment "Provides the display label to use for the unit of measurement of a Complex Observable Property."@en .


#################################################################
#    Classes
#################################################################

###  http://fdri.ceh.ac.uk/vocab/metadata/Agent
:Agent rdf:type owl:Class ;
       rdfs:subClassOf <http://www.w3.org/ns/dcat#Resource> ,
                       <http://www.w3.org/ns/prov#Agent> .


###  http://fdri.ceh.ac.uk/vocab/metadata/Annotation
:Annotation rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :hasValue ;
                              owl:minCardinality "0"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasValueSeries ;
                              owl:minCardinality "0"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :property ;
                              owl:cardinality "1"^^xsd:nonNegativeInteger
                            ] .


###  http://fdri.ceh.ac.uk/vocab/metadata/Catchment
:Catchment rdf:type owl:Class ;
           rdfs:subClassOf <http://www.w3.org/2004/02/skos/core#Concept> ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                             owl:allValuesFrom :CatchmentScheme
                           ] .


###  http://fdri.ceh.ac.uk/vocab/metadata/CatchmentScheme
:CatchmentScheme rdf:type owl:Class ;
                 rdfs:subClassOf <http://www.w3.org/2004/02/skos/core#ConceptScheme> .


###  http://fdri.ceh.ac.uk/vocab/metadata/ComplexObservableProperty
:ComplexObservableProperty rdf:type owl:Class ;
                           rdfs:subClassOf <http://www.w3.org/2004/02/skos/core#Concept> ,
                                           <http://www.w3.org/ns/sosa/ObservableProperty> ,
                                           <https://w3id.org/iadopt/ont/Variable> ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty :environmentalDomain ;
                                             owl:allValuesFrom :EnvironmentalDomain
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                             owl:allValuesFrom :ComplexObservablePropertyScheme
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty <https://w3id.org/iadopt/ont/hasContextObject> ;
                                             owl:allValuesFrom :ContextObject
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty <https://w3id.org/iadopt/ont/hasObjectOfInterest> ;
                                             owl:allValuesFrom :ObjectOfInterest
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty :hasValueScheme ;
                                             owl:minCardinality "0"^^xsd:nonNegativeInteger
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty :hasUnit ;
                                             owl:cardinality "1"^^xsd:nonNegativeInteger
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty :unitName ;
                                             owl:minCardinality "1"^^xsd:nonNegativeInteger
                                           ] ;
                           rdfs:comment "An Observable Property whose definition also captures additional information about the context, units, period and statistical aggregation of each measurement of the property."@en ;
                           rdfs:label "Complex Observable Property"@en .


###  http://fdri.ceh.ac.uk/vocab/metadata/ComplexObservablePropertyScheme
:ComplexObservablePropertyScheme rdf:type owl:Class ;
                                 rdfs:subClassOf <http://www.w3.org/2004/02/skos/core#ConceptScheme> .


###  http://fdri.ceh.ac.uk/vocab/metadata/ConfigurationItem
:ConfigurationItem rdf:type owl:Class ;
                   rdfs:subClassOf :ConfigurationValue ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty <http://purl.org/dc/terms/replaces> ;
                                     owl:allValuesFrom :ConfigurationItem
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :method ;
                                     owl:minCardinality "1"^^xsd:nonNegativeInteger
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :phenomenonInterval ;
                                     owl:minCardinality "0"^^xsd:nonNegativeInteger
                                   ] .


###  http://fdri.ceh.ac.uk/vocab/metadata/ConfigurationProperty
:ConfigurationProperty rdf:type owl:Class ;
                       rdfs:subClassOf <http://www.w3.org/2004/02/skos/core#Concept> ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                         owl:allValuesFrom :ConfigurationPropertyScheme
                                       ] .


###  http://fdri.ceh.ac.uk/vocab/metadata/ConfigurationPropertyScheme
:ConfigurationPropertyScheme rdf:type owl:Class ;
                             rdfs:subClassOf <http://www.w3.org/2004/02/skos/core#ConceptScheme> .


###  http://fdri.ceh.ac.uk/vocab/metadata/ConfigurationValue
:ConfigurationValue rdf:type owl:Class ;
                    rdfs:subClassOf :PropertyValue ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :interval ;
                                      owl:minCardinality "1"^^xsd:nonNegativeInteger
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty <http://purl.org/dc/terms/type> ;
                                      owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                                      owl:onClass <http://www.w3.org/2004/02/skos/core#Concept>
                                    ] .


###  http://fdri.ceh.ac.uk/vocab/metadata/ConfigurationValueSeries
:ConfigurationValueSeries rdf:type owl:Class ;
                          rdfs:subClassOf :PropertyValueSeries ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty <http://purl.org/dc/terms/type> ;
                                            owl:allValuesFrom :ConfigurationProperty
                                          ] .


###  http://fdri.ceh.ac.uk/vocab/metadata/ContextObject
:ContextObject rdf:type owl:Class ;
               rdfs:subClassOf <http://www.w3.org/2004/02/skos/core#Concept> ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                 owl:allValuesFrom :ContextObjectScheme
                               ] .


###  http://fdri.ceh.ac.uk/vocab/metadata/ContextObjectScheme
:ContextObjectScheme rdf:type owl:Class ;
                     rdfs:subClassOf <http://www.w3.org/2004/02/skos/core#ConceptScheme> .


###  http://fdri.ceh.ac.uk/vocab/metadata/DataProcessingConfiguration
:DataProcessingConfiguration rdf:type owl:Class ;
                             rdfs:subClassOf <http://www.w3.org/ns/dcat#Resource> ,
                                             <http://www.w3.org/ns/prov#Plan> ,
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty :appliesToSystem ;
                                               owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                               owl:onClass [ rdf:type owl:Class ;
                                                             owl:unionOf ( :EnvironmentalMonitoringSystem
                                                                           :EnvironmentalMonitoringSystemType
                                                                         )
                                                           ]
                                             ] ,
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty :appliesToVariable ;
                                               owl:minCardinality "1"^^xsd:nonNegativeInteger
                                             ] ,
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty :hadConfigurationItem ;
                                               owl:minCardinality "0"^^xsd:nonNegativeInteger
                                             ] ,
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty :hasCurrentConfigurationItem ;
                                               owl:minCardinality "0"^^xsd:nonNegativeInteger
                                             ] .


###  http://fdri.ceh.ac.uk/vocab/metadata/Deployment
:Deployment rdf:type owl:Class ;
            rdfs:subClassOf <http://www.w3.org/ns/ssn/Deployment> ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://www.w3.org/ns/ssn/deployedOnPlatform> ;
                              owl:allValuesFrom :EnvironmentalMonitoringPlatform
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://www.w3.org/ns/ssn/deployedSystem> ;
                              owl:allValuesFrom :EnvironmentalMonitoringSystem
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :deploymentVariance ;
                              owl:minCardinality "0"^^xsd:nonNegativeInteger
                            ] .


###  http://fdri.ceh.ac.uk/vocab/metadata/EnvironmentalDomain
:EnvironmentalDomain rdf:type owl:Class ;
                     rdfs:subClassOf <http://www.w3.org/2004/02/skos/core#Concept> ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                       owl:allValuesFrom :EnvironmentalDomainScheme
                                     ] .


